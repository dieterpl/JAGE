<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="vbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.141"
      fx:controller="JAGE.GUI.DebugWindowController">
    <SplitPane fx:id="pane" dividerPositions="0.5" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
               minWidth="-Infinity" prefHeight="100" prefWidth="100">
        <items>
            <TableView fx:id="dataTable" prefHeight="200.0" prefWidth="200.0">
                <columns>
                    <TableColumn fx:id="addressCol" prefWidth="75.0" text="Address"/>
                    <TableColumn fx:id="ramValueCol" prefWidth="75.0" text="Value"
                                 style="-fx-font-family: 'monospace'; -fx-font-size: 15"/>
                    <TableColumn fx:id="ramValueHCol" prefWidth="75.0" text="ValueH"
                                 style="-fx-font-family: 'monospace'"/>
                </columns>
            </TableView>
            <TableView fx:id="registerTable" prefHeight="200.0" prefWidth="200.0">
                <columns>
                    <TableColumn fx:id="registerCol" editable="false" prefWidth="200.0" text="Register"/>
                    <TableColumn fx:id="valueCol" editable="false" prefWidth="200.0" text="Value"/>
                </columns>
            </TableView>

        </items>
    </SplitPane>
    <ButtonBar minHeight="40.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button fx:id="updateBtn" mnemonicParsing="false" onAction="#update" text="Update"/>
        </buttons>
    </ButtonBar>
</VBox>
