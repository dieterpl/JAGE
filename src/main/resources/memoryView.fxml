<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.1" fx:controller="JAGE.GUI.MemoryView.MemoryViewController"
            style="-fx-font-family: 'monospace'; -fx-font-size: 13">

    <children>
        <TableView fx:id="memoryTable" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn editable="false" prefWidth="75.0" text="INDEX" fx:id="indexCol">

                    <cellValueFactory>
                        <PropertyValueFactory property="index"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" sortable="false" text="0" fx:id="off0Col">
                    <cellValueFactory>
                        <PropertyValueFactory property="off0"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="1" fx:id="off1Col">
                    <cellValueFactory>
                        <PropertyValueFactory property="off1"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="2" fx:id="off2Col">
                    <cellValueFactory>
                        <PropertyValueFactory property="off2"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="3" fx:id="off3Col">
                    <cellValueFactory>
                        <PropertyValueFactory property="off3"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="4" fx:id="off4Col">
                    <cellValueFactory>
                        <PropertyValueFactory property="off4"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="5" fx:id="off5Col">
                    <cellValueFactory>
                        <PropertyValueFactory property="off5"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="6" fx:id="off6Col">
                    <cellValueFactory>
                        <PropertyValueFactory property="off6"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="7" fx:id="off7Col">
                    <cellValueFactory>
                        <PropertyValueFactory property="off7"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="Text" fx:id="textCol">
                    <cellValueFactory>
                        <PropertyValueFactory property="text"/>
                    </cellValueFactory>
                </TableColumn>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
